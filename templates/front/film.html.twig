{% extends 'baseFront.html.twig' %}

{% block pageStyle %}<link href="{{ asset('css/index.css') }}" rel="stylesheet">
{% endblock %}

{% block body %}
	<body>

		{{ block("header") }}

		<section id="center" class="center_home">
			<div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel" data-bs-interval="6000">
				<div class="carousel-inner">
					{% for film in films %}
						<div class="carousel-item {% if loop.first %} active{% endif %}">
							<img src="{{ asset(film.image) }}" class="d-block w-100" alt="..." height="850">
							<div class="carousel-caption d-md-block">
								<h1 class="font_60">
									{{film.nom}}</h1>
								<h6 class="mt-3">
									<span class="col_red me-3">
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star-half-o"></i>
									</span>
									4.5 (Imdb)      Year: {{film.annederalisation}}
									<a class="bg_red p-2 pe-4 ps-4 ms-3 text-white d-inline-block" href="#">Action</a>
								</h6>
								<p= class="mt-3">{{film.description}}</p>
								<p class="mb-2">
									<span class="col_red me-1 fw-bold">category:</span>
									{% for category in film.categorys %}
										<a href="{{ path('app_category_show', {'id': category.id}) }}">{{ category.nom }}</a>
										{% if not loop.last %},
										{% endif %}
									{% endfor %} </h6></p>
								<p class="mb-2">
									<span class="col_red me-1 fw-bold">Actors:</span>
									{% for actor in film.actors %}
										<a href="{{ path('app_actor_show', {'id': actor.id}) }}">{{ actor.nom }}</a>
										{% if not loop.last %},
										{% endif %}
									{% endfor %}</p>
								<p>
									<span class="col_red me-1 fw-bold">Runtime:</span>
									{{film.duree|date('H:i:s')}}</p>
								<button class="button mt-4" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
									<i class="fa fa-play-circle align-middle me-1"></i>
									Watch Trailer</button>
							</div>
						</div>
					{% endfor %}
					<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
						<div class="modal-dialog">
							<div class="modal-content">
								<div class="modal-header">
									<h5 class="modal-title" id="staticBackdropLabel">Modal title</h5>
									<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
								</div>
								<div class="modal-body">
									<iframe width="560" height="315" src="https://www.youtube.com/embed/7wqatc6YNz0" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
								</div>
								<div class="modal-footer">
									<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
									<button type="button" class="btn btn-primary">Understood</button>
								</div>
							</div>
						</div>
					</div>
				</div>
				<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
					<span class="carousel-control-prev-icon" aria-hidden="true"></span>
					<span class="visually-hidden">Previous</span>
				</button>
				<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
					<span class="carousel-control-next-icon" aria-hidden="true"></span>
