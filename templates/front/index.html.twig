							<img src="{{ asset(film.image) }}" class="d-block w-100" alt="..." style="height: 80vh;" alt="...">
							<div class="carousel-caption d-md-block">
								<h1 class="font_60">{{ film.nom }}</h1>
								<h6 class="mt-3">
									<span class="col_red me-3">
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star-half-o"></i>
