									Year :
									{{ film.annederalisation }}
									<a class="bg_red p-2 pe-4 ps-4 ms-3 text-white d-inline-block" href="#">
										{% for category in film.categorys %}
											{{ category.nom }}
											{% if not loop.last %},
											{% endif %}
										{% endfor %}
									</a>
								</h6>
								<p class="mt-3">{{ film.description }}</p>
								<p class="mb-2">
									<span class="col_red me-1 fw-bold">Starring:</span>
									{% for actor in film.actors %}
										{{ actor.nom }}
										{% if not loop.last %},
										{% endif %}
									{% endfor %}
								</p>
								<p>
									<span class="col_red me-1 fw-bold">Runtime:</span>
									{{ film.duree|date('H:i') }}
								</p>
								<h6 class="mt-4">
									<a class="button" href="{{path('app_filShow_index',{'id':film.id})}}" style="margin-left: 66px;">
										<i class="fa fa-play-circle align-middle me-1"></i>
										Read More</a>
								</h6>
							</div>
						</div>
					{% endfor %}
