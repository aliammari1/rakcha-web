{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.5.4"
    }
  },
  "routes": [
    {
      "src": "/(assets)/(.*)$",
      "dest": "/api/assets.php?path=$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "prod",
    "APP_DEBUG": "true"
  }
}
